{
  "name": "MobileCRM",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "reinstall": "rm -rf node_modules && npm install && cd ios && rm -rf Pods && pod install",
    "test": "jest __tests__/",
    "code-push-deploy": "./code-push-deploy.sh",
